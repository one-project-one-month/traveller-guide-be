{
    "name": "traveller-guide",
    "version": "1.0.0",
    "main": "dist/server.js",
    "license": "MIT",
    "scripts": {
        "watch": "tsc --watch",
        "start": "node dist/server.js",
        "start:dev": "nodemon dist/server.js",
        "lint": "eslint . --ext .ts,.tsx,.js,.mjs",
        "build": "npm run lint && tsc",
        "build:docs": "node scripts/build-docs.js",
