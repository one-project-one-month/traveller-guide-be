        "@types/express": "^5.0.3",
        "@types/express-rate-limit": "^6.0.2",
        "@types/js-yaml": "^4.0.9",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/lodash": "^4.17.20",
        "@types/morgan": "^1.9.10",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.5.2",
        "@types/swagger-ui-express": "^4.1.8",
        "eslint": "^9.36.0",
        "globals": "^16.4.0",
        "nodemon": "^3.1.10",
        "pino-pretty": "^13.1.1",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.1"
