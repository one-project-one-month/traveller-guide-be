        "watch:docs": "nodemon --watch swagger --ext yaml,yml --exec \"npm run build:docs\""
    },
    "dependencies": {
        "@neondatabase/serverless": "^1.0.1",
        "@prisma/client": "^6.16.2",
        "bcryptjs": "^3.0.2",
