        "cloudinary": "^2.7.0",
        "compression": "^1.8.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dayjs": "^1.11.18",
        "dotenv": "^17.2.2",
        "express": "^4.21.2",
        "express-rate-limit": "^8.1.0",
        "helmet": "^8.1.0",
        "http-status": "^2.1.0",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "pino": "^9.11.0",
        "prisma": "^6.16.2",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.1.11"
